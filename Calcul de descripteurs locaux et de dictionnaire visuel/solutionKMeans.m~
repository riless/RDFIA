function [centres,erreur] = solutionKMeans(points,M)
    
   [N,d] = size(points); 
   % M : nombre de clusters voulues
   % centres: les coordonn√©e du centre ( le sift du centre )
   % erreur: erreur de quantification obtenu
   
   %% Initialisation des centres
   K = M;
   centres = randomSeed(points,K);
   
   %% tant que les centres ne s
   
  

end